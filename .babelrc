{
    "presets": [
      ["env", {
        "targets": {
          "ie":  "10",
          "browsers": ["last 2 versions"]
        }
      }],
  ],
  "plugins": [
      "transform-decorators-legacy",
      "syntax-dynamic-import",
      ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
          }
      ]
  ]
}
